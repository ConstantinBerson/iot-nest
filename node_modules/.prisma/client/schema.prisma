datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  developerId       String @id 
  consumptionLimite Int
  devices           Devices[]
}

model Devices {
  id          String @id
  online      Boolean
  status      Boolean
  user        User @relation(fields: [developerId], references: [developerId])
  developerId String
}

